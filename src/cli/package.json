{
  "name": "su-covid-daily-cli",
  "version": "1.0.4",
  "description": "Automatically report your daily COVID symptom form to the Wellness Center.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepare": "npm run build",
    "link": "npm link ../form-automation",
    "build": "tsc && cd ../form-automation && npm run build",
    "prestart": "npm run build",
    "start": "node dist/index.js"
  },
  "bin": {
    "sucovid": "./dist/index.js"
  },
  "preferGlobal": true,
  "files": [
    "dist/**/*"
  ],
  "author": "Sean Dodson",
  "license": "ISC",
  "dependencies": {
    "commander": "^6.0.0",
    "inquirer": "^7.3.3",
    "keytar": "^6.0.1",
    "moment": "^2.27.0",
    "node-cron": "^2.0.3",
    "su-covid-daily": "^1.0.4"
  },
  "devDependencies": {
    "@types/inquirer": "^7.3.1",
    "@types/node": "^14.6.0",
    "@types/node-cron": "^2.0.3",
    "typescript": "^4.0.2"
  }
}
