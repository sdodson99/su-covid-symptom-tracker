  "bin": {
    "sucovid": "./dist/index.js"
  },
  "preferGlobal": true,
  "files": [
    "dist/**/*"
  ],
  "author": "Sean Dodson",
  "license": "ISC",
  "dependencies": {
    "su-covid-daily": "file:../form-automation"
  },
  "devDependencies": {
    "@types/node": "^14.6.0",
    "typescript": "^4.0.2"
  }
}
