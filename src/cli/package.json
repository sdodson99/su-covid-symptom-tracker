{
  "name": "su-covid-daily-cli",
  "version": "1.0.0",
  "description": "Automatically report your daily COVID symptom form to the Wellness Center.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc && cd ../form-automation && npm run build",
