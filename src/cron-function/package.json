    "@typescript-eslint/parser": "^4.0.1",
    "eslint": "^7.7.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-config-node": "^4.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^2.1.1",
    "typescript": "^3.3.3"
  },
